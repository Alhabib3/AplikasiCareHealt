<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aplikasicarehealt">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AplikasiCareHealt">

        <!-- MainActivity as launcher activity -->
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- LoginActivity -->
        <activity android:name="com.millenialzdev.aplikasicarehealt.LoginActivity" />

        <!-- RegisterActivity -->
        <activity android:name=".RegisterActivity" />

        <!-- HalamanUtamaActivity -->
        <activity android:name=".HalamanUtamaActivity" />

        <!-- PolatidurActivity -->
        <activity android:name=".PolatidurActivity" />

        <activity android:name=".PolamakanActivity" />

        <activity android:name=".JadwalOlahragaActivity"/>

        <activity android:name=".ObatActivity" />

        <activity android:name=".VitaminActivity" />

    </application>

</manifest>
